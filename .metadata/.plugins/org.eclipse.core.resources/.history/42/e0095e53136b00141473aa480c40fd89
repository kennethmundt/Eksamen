package application;

public class Animal
{
    private String animal;
    private String animalName;
    private String animalBirth;
    
    public Animal(String animal, String animalName, String animalAge)
    {
	this.animal = animal;
	this.animalName = animalName;
	this.animalBirth = animalAge;
    }

    public String getAnimal()
    {
        return animal;
    }

    public String getAnimalName()
    {
        return animalName;
    }

    public String getAnimalBirth()
    {
        return animalBirth;
    }

    public void setAnimal(String animal)
    {
        this.animal = animal;
    }

    public void setAnimalName(String animalName)
    {
        this.animalName = animalName;
    }

    public void setAnimalBirth(String animalBirth)
    {
        this.animalBirth = animalBirth;
    }

    @Override
    public String toString()
    {
	return "Animal [animal=" + animal + ", animalName=" + animalName
		+ ", animalAge=" + animalBirth + "]";
    }
}